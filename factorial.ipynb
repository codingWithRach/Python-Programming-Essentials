   "outputs": [],
   "source": [
    "def getFactorial(n):\n",
    "    if n < 0:         # validation\n",
    "        return None\n",
    "    if n < 2:         # base case\n",
    "        return 1\n",
    "    return n * getFactorial(n-1)"
   ]
  },
